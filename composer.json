    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "AllYouNeed\\Seo\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "statamic/cms": "^4.0|^5.0|^6.0"
    },
    "extra": {
