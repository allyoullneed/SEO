            "name": "Alt Seo",
            "description": "Alt Seo addon"
        },
        "laravel": {
            "providers": [
                "AllYouNeed\\Seo\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": false
        }
    }
}
